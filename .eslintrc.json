{
  "extends": ["plugin:react/recommended", "standard"],
  "parser": "babel-eslint",
  "plugins": ["react", "babel"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true
  },
  "rules": {
    "import/first": "off",
    "react/display-name": 0,
    "react/prop-types": 0,
    "one-var": 0,
    "no-unused-vars": 1,
    "no-tabs": 1,
    "indent": "error",
    "func-call-spacing": "off",
    "no-unexpected-multiline": "off",
    "no-trailing-spaces": 0,
    "no-mixed-operators": 0
  },
  "globals": {
    "i18n": true,
    "Raven": true,
    "ga": true
  }
}
